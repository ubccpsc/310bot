{
  "name": "bot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "engines": {
    "node": ">= 16 < 17"
  },
  "dependencies": {
    "@ubccpsc310/bot-base": "^1.0.7",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^16.7.10",
    "nodemon": "2.0.12",
    "ts-node": "10.2.1",
    "typescript": "4.4.2"
  },
  "scripts": {
    "start": "ts-node src/App.ts",
    "watch": "nodemon"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": ".ts"
  }
}
